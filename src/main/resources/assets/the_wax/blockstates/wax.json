{
  "multipart": [
    {
      "apply": {
        "model": "the_wax:block/wax"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax",
        "uvlock": true,
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_top"
      },
      "when": {
        "in_top": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_ground"
      },
      "when": {
        "on_ground": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_head"
      },
      "when": {
        "head": "true"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside",
        "y": 90
      },
      "when": {
        "east": "false"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside",
        "y": 180
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside",
        "x": 270
      },
      "when": {
        "up": "false"
      }
    },
    {
      "apply": {
        "model": "the_wax:block/wax_inside",
        "x": 90
      },
      "when": {
        "down": "false"
      }
    }
  ]
}